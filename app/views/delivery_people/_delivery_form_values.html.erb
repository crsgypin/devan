<%# delivery_person, form_values %>

<h2> <%= delivery_person.name %> </h2>

<table class="table table-bordered">
	<tr>
		<td>日期</td>
		<td>店名</td>
	</tr>
	<% form_values.each do |form_value| %>
		<tr>
			<td> <%= form_value.daily_form.try(:date) %> </td>
			<td> <%= form_value.customer.try(:name) %> </td>
		</tr>
	<% end %>
</table>



